{
  "name": "random-project-name-generator-frontend",
  "description": "The frontend for the random project name generator",
  "repository": "https://github.com/christophstach/random-project-name-generator/tree/master/frontend",
  "version": "1.4.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:ssr",
    "start:dev": "ng serve --aot",
    "start:prod": "ng serve --aot --prod",
    "start:ssr": "node dist/server.js",
    "start:prerender": "cd dist/browser && http-server",
    "build:dev": "ng build --aot",
    "build:prod": "ng build --aot --prod --build-optimizer",
    "build:client-and-server-bundles": "ng build --prod && ng run frontend:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "graqphql:introspect": "npx apollo schema:download --endpoint=http://localhost:3000/graphql",
    "graqphql:generate:types": "npx apollo codegen:generate --queries=./src/**/*.ts --schema=./schema.json --target=typescript --outputFlat=./src/schema-types.ts",
    "postinstall": "npm run build:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/platform-server": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@auth0/angular-jwt": "^2.0.0",
    "@mdi/font": "^2.5.94",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@ngxs/devtools-plugin": "^3.2.0",
    "@ngxs/logger-plugin": "^3.2.0",
    "@ngxs/router-plugin": "^3.2.0",
    "@ngxs/storage-plugin": "^3.2.0",
    "@ngxs/store": "^3.2.0",
    "@fortawesome/fontawesome-free": "^5.2.0",
    "apollo-angular": "^1.1.2",
    "apollo-angular-link-http": "^1.1.1",
    "apollo-cache-inmemory": "^1.2.6",
    "apollo-client": "^2.3.7",
    "apollo-codegen": "^0.20.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-error": "^1.1.0",
    "core-js": "^2.5.4",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "install": "^0.12.1",
    "moment": "^2.22.2",
    "ngx-materialize": "^6.0.1",
    "rxjs": "^6.0.0",
    "ts-loader": "^4.4.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.1.1",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.5.3",
    "codelyzer": "^4.4.2",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.2.0",
    "protractor": "^5.4.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "~2.9.2",
    "webpack-cli": "^3.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
